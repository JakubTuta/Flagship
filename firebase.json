{
  "hosting": {
    "public": ".output/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "server",
        "region": "europe-central2"
      }
    ]
  },
  "functions": {
    "predeploy": "bun run build",
    "source": ".output/server",
    "runtime": "nodejs20",
    "region": "europe-central2"
  }
}